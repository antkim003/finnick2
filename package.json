    "start": "node server/start",
    "start-dev": "nodemon --ignore browser/ --ignore src/ --watch server -e js server/start.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build-stage": "rimraf dist && cross-env NODE_ENV=development webpack --config ./webpack.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "test-frontend:watch": "mocha --compilers js:babel-core/register --require ./tests/browser/test_helper.js 'tests/browser/**/*.js' --watch",
    "test-server": "cross-env TESTING=testing node node_modules/.bin/mocha --compilers js:babel-register --recursive tests/server/**/*.js || true",
    "test-routes": "cross-env TESTING=testing mocha --compilers js:babel-register tests/server/routes/* || true",
    "test-moverow": "cross-env TESTING=testing mocha --compilers js:babel-register tests/server/routes/moverows-test || true",
    "jscs": "jscs ."
  },
